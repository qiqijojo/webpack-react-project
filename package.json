{
    "name": "react-scaffold",
    "version": "1.0.0",
    "description": "webpack-react-peoject",
    "scripts": {
        "start": "webpack-dev-server --config webpack/webpack.config.development.js --color",
        "build": "webpack --config webpack/webpack.config.production.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qiqijojo/webpack-react-project.git"
    },
    "keywords": [
        "webpack\\react"
    ],
    "author": "JOJO",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/qiqijojo/webpack-react-project/issues"
    },
    "homepage": "https://github.com/qiqijojo/webpack-react-project#readme",
    "dependencies": {
        "antd": "^3.15.1",
        "react": "^16.8.3",
        "react-dom": "^16.8.3"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "eslint": "^5.15.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "prettier": "^1.16.4",
        "stylelint": "^9.10.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
