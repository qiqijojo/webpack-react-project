{
    "name": "react-scaffold",
    "version": "1.0.0",
    "description": "webpack-react-peoject",
    "scripts": {
        "start": "webpack-dev-server --config webpack/webpack.config.development.js --color",
        "build": "webpack --config webpack/webpack.config.production.js",
        "build:dll": "webpack --config webpack/webpack.config.dll.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qiqijojo/webpack-react-project.git"
    },
    "keywords": [
        "webpack\\react"
    ],
    "author": "JOJO",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/qiqijojo/webpack-react-project/issues"
    },
    "homepage": "https://github.com/qiqijojo/webpack-react-project#readme",
    "dependencies": {
        "antd": "^3.15.1",
        "history": "^4.9.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-loadable": "^5.5.0",
        "react-router": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-import": "^1.11.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^2.1.1",
        "cssnano": "^4.1.10",
        "eslint": "^5.15.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "html-webpack-include-assets-plugin": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "image-webpack-loader": "^4.6.0",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "lint-staged": "^8.1.5",
        "mini-css-extract-plugin": "^0.5.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.6.0",
        "prettier": "^1.16.4",
        "style-loader": "^0.23.1",
        "stylelint": "^9.10.1",
        "terser-webpack-plugin": "^1.3.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.6",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1"
    },
    "lint-staged": {
        "linters": {
            "src/**/*.{js, jsx}": [
                "eslint --fix",
                "git add"
            ],
            "*.{json, md}": [
                "prettier --write",
                "git add"
            ]
        },
        "ignore": [
            "package.json"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "browserslist": [
        "> 1% in CN",
        "last 4 versions",
        "Firefox ESR",
        "not ie < 9"
    ]
}
