{
  "name": "react-scaffold",
  "version": "1.0.0",
  "description": "webpack-react-peoject",
  "scripts": {
    "lint": "eslint \"src/*.js\"",
    "start": "webpack-dev-server --config webpack/webpack.config.development.js",
    "build": "webpack --config webpack/webpack.config.production.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiqijojo/webpack-react-project.git"
  },
  "keywords": [
    "webpack\\react"
  ],
  "author": "JOJO",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qiqijojo/webpack-react-project/issues"
  },
  "homepage": "https://github.com/qiqijojo/webpack-react-project#readme",
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
