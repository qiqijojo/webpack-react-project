{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "no-console": [
            "error",
            {
                "allow": ["warn", "error", "info"]
            }
        ],
        "comma-dangle": 0,
        "react/jsx-indent": [1, 4],
        "indent": [1, 4],
        "no-unused-vars": [1],
        "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
    }
}
